<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.1//EN" "http://www.netbeans.org/dtds/filesystem-1_1.dtd">
<filesystem>
    <folder name="Editors">
        <folder name="text">
            <folder name="base">
                <file name="EditorKit.instance">
                    <attr name="instanceClass" stringvalue="org.netbeans.modules.jvi.ViKit"/>
                </file>
            </folder>

            <folder name="x-java">
                <file name="EditorKit.instance">
                    <attr name="instanceClass" stringvalue="org.netbeans.modules.jvi.JavaViKit"/>
                </file>
            </folder>

            <folder name="html">
                <file name="EditorKit.instance">
                    <attr name="instanceClass" stringvalue="org.netbeans.modules.jvi.HtmlViKit"/>
                </file>
            </folder>

            <folder name="plain">
                <file name="EditorKit.instance">
                    <attr name="instanceClass" stringvalue="org.netbeans.modules.jvi.PlainViKit"/>
                </file>
            </folder>
            <folder name="xml">
                <file name="EditorKit.instance">
                    <attr name="instanceClass" stringvalue="org.netbeans.modules.jvi.XMLViKit"/>
                </file>
            </folder>
        </folder>
    </folder>
    <folder name="UI">
        <folder name="Services">
            <folder name="Editing">
                <file name="NbOptions.instance">
                    <attr name="instanceCreate" newvalue="org.netbeans.modules.jvi.NbOptionsNode"/>
                    <attr name="instanceClass" stringvalue="org.openide.nodes.Node"/>
                    <attr name="name" stringvalue="Vi Options"/>
                </file>
            </folder>
        </folder>
    </folder>
</filesystem>
